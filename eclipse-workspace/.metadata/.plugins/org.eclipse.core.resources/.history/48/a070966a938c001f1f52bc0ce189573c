public class TravelAgencyApplication {

	public static void main(String[] args) {
		SpringApplication.run(TravelAgencyApplication.class, args);
	}
	
	@RequestMapping("/")
	public String greet(){
		String myText = "Hello";
		
	   return myText;
	}

}
